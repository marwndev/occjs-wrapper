{
    "name": "occjs-wrapper",
    "version": "2.0.0-beta.b5ff984-7",
    "author": "Marwan Aouida <contact@marwan.dev>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marwndev/occjs-wrapper.git"
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build:dev": "NODE_OPTIONS=--max-old-space-size=8000 tsx src/main.ts",
        "build": "NODE_OPTIONS=--max-old-space-size=8000 tsx src/main.ts && npm run minify",
        "minify": "npx terser dist/occjs.js --module --mangle --compress -o dist/occjs.js"
    },
    "devDependencies": {
        "@types/node": "^24.0.7",
        "ts-morph": "^26.0.0",
        "tsx": "^4.20.3",
        "typescript": "~5.8.3",
        "terser": "^5.43.1"
    },
    "dependencies": {}
}
